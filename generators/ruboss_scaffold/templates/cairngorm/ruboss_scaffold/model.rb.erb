class <%= class_name %> < ActiveRecord::Base
<% for model in has_ones -%>
  has_one :<%= model %>, :dependent => :destroy
<% end -%>
<% for model in has_manies -%>
  has_many :<%= model %>, :dependent => :delete_all
<% end -%>
<% for model in belongs_tos -%>
  belongs_to :<%= model %>
<% end -%>
end
